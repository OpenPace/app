{
  "expo": {
    "name": "OpenPace",
    "slug": "openpace-challenges",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "openpace",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#515151"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.openpace.challenges",
      "buildNumber": "1.2.0",
      "associatedDomains": [
        "applinks:*.openpace.co"
      ]
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#515151"
    },
    "android": {
      "package": "com.openpace.challenges",
      "versionCode": 380010200,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foregroundImage.png",
        "backgroundImage": "./assets/images/backgroundImage.png"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.openpace.co",
              "pathPrefix": "/invite"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    }
  }
}
