{
  "expo": {
    "name": "OpenPace",
    "slug": "openpace-challenges",
    "version": "1.4.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "openpace",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#515151"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.openpace.challenges",
      "usesAppleSignIn": true,
      "buildNumber": "1.4.1",
      "associatedDomains": [
        "applinks:*.openpace.co"
      ],
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#515151"
    },
    "android": {
      "package": "com.openpace.challenges",
      "versionCode": 400010401,
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_INTERNAL_STORAGE",
        "android.permission.RECEIVE_BOOT_COMPLETED"
      ],
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foregroundImage.png",
        "backgroundImage": "./assets/images/backgroundImage.png"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.openpace.co",
              "pathPrefix": "/invite"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    }
  }
}
